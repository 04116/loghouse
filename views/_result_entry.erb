<span class="logs-result__entry-timestamp"><%= entry.timestamp.strftime("%Y-%m-%d %H:%M:%S.%N") %></span>

<% LoghouseQuery::KUBERNETES_ATTRIBUTES.keys.each do |k| %>
  <span data-key="<%= k %>">
    <span class="logs-result__entry-kube-key"><%= k %>:</span> <span class="logs-result__entry-value"><%= entry.send(k) %></span>
  </span>
<% end %>

<% entry.strings.each do |k, v| %>
  <span data-key="<%= k %>">
    <span class="logs-result__entry-key"><%= k %>:</span> <span class="logs-result__entry-value"><%= v %></span>
  </span>
<% end %>

<% entry.numbers.each do |k, v| %>
  <span data-key="<%= k %>">
    <span class="logs-result__entry-key"><%= k %>:</span> <span class="logs-result__entry-value"><%= v %></span>
  </span>
<% end %>

<% entry.booleans.each do |k, v| %>
  <span data-key="<%= k %>">
    <span class="logs-result__entry-key"><%= k %>:</span> <span class="logs-result__entry-value"><%= v ? 'true' : 'false' %></span>
  </span>
<% end %>

<% entry.nulls.each do |k| %>
  <span data-key="<%= k %>">
    <span class="logs-result__entry-key"><%= k %>:</span> <span class="logs-result__entry-value">NULL</span>
  </span>
<% end %>
